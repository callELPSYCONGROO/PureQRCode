<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wuhenjian.pureqrcode">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_img"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".subactivity.CreateQRActivity" android:label="@string/lable_create">
        </activity>

        <activity android:name=".subactivity.AnalysisQRActivity" android:label="@string/lable_analysis">
        </activity>
    </application>

</manifest>